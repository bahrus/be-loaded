{
  "name": "be-loaded",
  "version": "0.0.48",
  "description": "be-loaded is a web component decorator that allows a web component to import resources configured via index.html",
  "main": "be-loaded.js",
  "module": "be-loaded.js",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements"
  ],
  "exports": {
    ".": "./be-loaded.js",
    "./be-loaded.js": "./be-loaded.js",
    "./importCSS.js": "./importCSS.js",
    "./importJSON.js": "./importJSON.js"
  },
  "files": [
    "*.*"
  ],
  "types": "types.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node node_modules/xtal-test/serve.js",
    "package-check": "package-check",
    "doc": "custom-elements-manifest analyze"
  },
  "dependencies": {
    "xtal-element": "0.0.480",
    "trans-render": "0.0.489",
    "be-decorated": "0.0.87",
    "be-hive": "0.0.34"
  },
  "devDependencies": {
    "xtal-test": "0.0.55",
    "@skypack/package-check": "0.2.2",
    "@custom-elements-manifest/analyzer": "0.5.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-loaded.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/be-loaded/issues"
  },
  "homepage": "https://github.com/bahrus/be-loaded#readme"
}
