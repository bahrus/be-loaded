{
  "name": "be-loaded",
  "version": "0.0.88",
  "description": "be-loaded is a web component decorator that allows a web component to import resources configured via index.html",
  "main": "be-loaded.js",
  "module": "be-loaded.js",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "preload",
    "css-modules"
  ],
  "exports": {
    ".": "./be-loaded.js",
    "./be-loaded.js": "./be-loaded.js",
    "./importCSS.js": "./importCSS.js",
    "./importJSON.js": "./importJSON.js"
  },
  "files": [
    "*.*",
    "demo/*.*"
  ],
  "types": "types.d.ts",
  "scripts": {
    "test": "playwright test",
    "serve": "node node_modules/may-it-serve/serve.js",
    "package-check": "package-check",
    "doc": "custom-elements-manifest analyze"
  },
  "dependencies": {
    "xtal-element": "0.0.530",
    "trans-render": "0.0.626",
    "be-decorated": "0.0.179",
    "be-hive": "0.0.74",
    "be-preemptive": "0.0.17"
  },
  "devDependencies": {
    "may-it-serve": "0.0.3",
    "@playwright/test": "1.27.1",
    "@skypack/package-check": "0.2.2",
    "@custom-elements-manifest/analyzer": "0.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-loaded.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/be-loaded/issues"
  },
  "homepage": "https://github.com/bahrus/be-loaded#readme"
}
